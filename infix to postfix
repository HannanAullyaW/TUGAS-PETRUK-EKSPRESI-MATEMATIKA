#include <bits/stdc++.h>
using namespace std;

bool oibreoir(char n){
    if(n == '+' || 
       n == '-' || 
       n == '*' || 
       n == '/' || 
       n == '%'   ){
        return true;
    }
    else{
        return false;
    }
}
int prioritet(string n){
    if(n == "+" ||
       n == "-"   ){
        return 1;
    }
    else if(n == "*" ||
            n == "/" ||
            n == "%"   ){
        return 2;
    }
    return 0;
}
